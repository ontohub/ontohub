<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	  @external .label, .label-primary;
	  .label-primary {}
	  .label {
	    display:block;
	    float: left;
	    position:relative;
	    font-size:14px;
	    height: 20px;
	    line-height:14px;
	    padding:3px 8px;
	    margin: 0 2px 0 0;
	    border-radius: 10px;
	  }
      .label:focus {
         outline: 0px none;
         background-color: rgb(44, 117, 180);
       }
	</ui:style>
	<g:FocusPanel styleName="label label-primary" ui:field="panel">
	  <g:InlineLabel styleName="" ui:field="label" />
	</g:FocusPanel>
</ui:UiBinder> 