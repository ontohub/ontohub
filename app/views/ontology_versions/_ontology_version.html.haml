%tr
  %td= link_to ontology_version.number, fancy_repository_path(resource_chain[0], path: resource_chain[1].path, ref: ontology_version.commit_oid, action: :download)
  %td= link_to ontology_version.commit_oid[0..8], repository_ref_path(ontology_version.repository, ontology_version.commit_oid, :diff)
  %td= timestamp ontology_version
  %td= pusher_info(ontology_version)
  %td= render partial: 'shared/state_tag', locals: {resource: ontology_version}
  %td
    = link_to 'Download', fancy_repository_path(resource_chain[0], path: ontology_version.path, ref: ontology_version.commit_oid, action: :download), class: 'btn btn-xs btn-default'
    = edit_or_view_button(ontology_version)
